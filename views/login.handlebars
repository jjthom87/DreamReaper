<form action="/users/login" method="post">
  <h1>Hello Dreamers!</h1>
  <h2>Login to Your Account Here</h2>
  <div>
      <label>Email</label>
      <br>
      <input type="text" name="email"/>
  </div>
  <div>
      <label>Password</label>
      <br>
      <input type="password" name="password"/>
  </div>
  <div>
      <input type="submit" value="Log In"/>
  </div>
</form>